<?xml version="1.0" encoding="utf-8"?>
<project name="GettingStarted Example" default="build" basedir="../../">

	<property file="build.properties"/>
	
	<target name="build">
		<exec executable="${mxmlc}" dir="${basedir}/examples/GettingStarted" failonerror="true">
			<arg line="--library-path+='${build.dir}/${library.shortName}.swc'"/>
			<arg line="GettingStarted.mxml"/>
		</exec>
	</target>
</project>